{
    "compilerOptions": {
        "allowJs": true,
        "baseUrl": ".",
        "paths": {
            "@/*": ["src/*"],
            "@/img/*": ["src/img/*"]
        },
        "esModuleInterop": true,
        "sourceMap": true,
        "jsx": "react",
        "experimentalDecorators": true,
        "lib": ["dom", "es2017"],
        "outDir": "./build"
    },
    "exclude": ["build", ".git", "node_modules"]
}
